{
    "name": "omatamix/container",
    "description": "A psr-11 compatible container.",
    "homepage": "https://github.com/omatamix/framework",
    "keywords": [],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/omatamix/framework/issues",
        "source": "https://github.com/omatamix/framework"
    },
    "require": {
        "psr/container": "^1.0|^2.0"
    },
    "provides": {
        "psr/container-implementation": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Omatamix\\Container\\": "./src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

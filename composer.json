{
    "name": "kooser/letty",
    "description": "A secure by default PHP framework built for speed and security.",
    "homepage": "https://github.com/Kooser06/Letty",
    "keywords": [],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Kooser06/Letty/issues",
        "source": "https://github.com/Kooser06/Letty"
    },
    "require": {
        "psr/cache": "^1.0",
        "psr/container": "^2.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.1",
        "psr/simple-cache": "^1.0"
    },
    "replace": {
        "kooser/cache": "self.version",
        "kooser/client": "self.version",
        "kooser/container": "self.version",
        "kooser/event": "self.version",
        "kooser/factory": "self.version",
        "kooser/log": "self.version",
        "kooser/middleware": "self.version",
        "kooser/response": "self.version",
        "kooser/server": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Letty\\": "./src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "roave/security-advisories": "dev-latest"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
